      ],
      "symbols": {
        "F": {"type": "draw", "color": "#006400", "length": 5},
        "X": {"type": "draw", "color": "#006400", "length": 0},
        "+": {"type": "turn", "angle": 25},
        "-": {"type": "turn", "angle": -25},
        "[": {"type": "push"},
        "]": {"type": "pop"}
      },
      "iterations": 5
    },
    
    {
      "name": "Fractal Vine",
      "axiom": "F",
      "rules": [
        {"a": "F", "b": "F+F--F+F"}
      ],
      "symbols": {
