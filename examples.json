        "+": {"type": "turn", "angle": 72},
        "-": {"type": "turn", "angle": -72}
      },
      "iterations": 3
    },
  
    {
      "name": "Hilbert Variant",
      "axiom": "A",
      "rules": [
        {"a": "A", "b": "-BF+AFA+FB-"},
        {"a": "B", "b": "+AF-BFB-FA+"}
      ],
      "symbols": {
        "F": {"type": "draw", "color": "#9400d3", "length": 4},
        "+": {"type": "turn", "angle": 90},
        "-": {"type": "turn", "angle": -90}
      },
      "iterations": 4
    },
    {
      "name": "Sierpinski Arrowhead",
      "axiom": "A",
      "rules": [
        {"a": "A", "b": "B-A-B"},
