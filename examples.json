      ],
      "symbols": {
        "F": {"type": "draw", "color": "#0000ff", "length": 3},
        "+": {"type": "turn", "angle": 90},
        "-": {"type": "turn", "angle": -90}
      },
      "iterations": 3
    },
    {
      "name": "Levy C Curve",
      "axiom": "F",
      "rules": [
        {"a": "F", "b": "+F--F+"}
      ],
      "symbols": {
        "F": {"type": "draw", "color": "#ff69b4", "length": 8},
        "+": {"type": "turn", "angle": 45},
        "-": {"type": "turn", "angle": -45}
      },
      "iterations": 7
    },
    {
      "name": "Quadratic Koch Island",
      "axiom": "F+F+F+F",
